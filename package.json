    "autoprefixer": "^10.2.6",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "mini-css-extract-plugin": "^2.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.4",
    "postcss-loader": "^8.1.1",
    "sass-embedded": "^1.80.5",
